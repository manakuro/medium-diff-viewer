{
    "manifest_version": 2,

    "name": "英文解釈SVO",
    "description": "英文をSVOに分類します。",
    "version": "1.0.0",

    "browser_action": {
        "default_icon": "icon16.png"
    },

    "background": {
        "scripts": ["js/eventPage.js"],
        "persistent": false
    },

    "content_scripts": [
      {
        "matches": [ "http://*/*", "https://*/*" ],
        "js": ["js/content.js"]
      }
    ],

    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },

    "permissions": [
      "tabs",
      "storage"
    ]
}
